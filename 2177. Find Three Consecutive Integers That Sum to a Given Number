class Solution {
public:
    vector<long long> sumOfThree(long long num) {
        vector<long long> arr;
        long long new_number=num/3;
        if(num%3==0){
            arr.push_back(new_number-1);
            arr.push_back(new_number);
            arr.push_back(new_number+1);
        }
        cout<<new_number;
        return arr;
    }
};
